{
  "extends": [
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "linebreak-style": 0,
    "import/prefer-default-export": 0,
    "@typescript-eslint/camelcase": 0,
    "arrow-parens": 0,
    "lines-between-class-members": 0,
    "import/extensions": 0,
    "max-classes-per-file": 0,
    "class-methods-use-this": 0,
    "import/no-cycle": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-mixed-operators": 0,
    "no-bitwise": 0
  }
}